<div class="container mt-5">

  <%= simple_form_for [@list, @bookmark] do |f| %>
    <%= f.input :comment %>
    <%= f.association :movie %>

    <%= f.button :submit, "Soumettre", class: "btn-outline-primary" %>
    <%= link_to "Cancel", list_path(@list), class: "btn-outline-secondary" %>
  <% end %>

</div>